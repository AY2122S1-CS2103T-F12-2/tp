<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>

<?import javafx.scene.layout.FlowPane?>
<StackPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" alignment="TOP_LEFT">
   <children>
      <TabPane fx:id="tabPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="200" minWidth="200" prefHeight="300.0" prefWidth="600.0" styleClass="tab-pane" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab fx:id="academic" text="Academic">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Label fx:id="institution" layoutY="5.0" text="Label" />
                        <Label fx:id="course" layoutY="76.0" text="Label" />
                        <Label fx:id="yearOfGrad" layoutY="231.0" text="Label" />
                        <Label fx:id="grade" layoutY="154.0" text="Label" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
            <Tab fx:id="skill" text="Skill">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                         <FlowPane fx:id="skills" />
                     </children>
                  </AnchorPane>
              </content>
            </Tab>
        </tabs>
      </TabPane>
   </children>
</StackPane>
